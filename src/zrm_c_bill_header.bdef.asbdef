projection;
strict ( 2 );
use draft;

define behavior for zrm_c_bill_header
{
  use create;
  use update;
  use delete;

  use action Edit;
  use action Resume;
  use action Activate;
  use action Discard;
  use action Prepare;

  use association _BillItem { create; with draft;}
}

define behavior for zrm_c_bill_item
{
  use update;
  use delete;

  use association _BillHeader { with draft; }
}